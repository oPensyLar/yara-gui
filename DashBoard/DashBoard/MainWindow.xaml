                    <Rectangle.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleY="1.038" ScaleX="0.963"/>
                            <SkewTransform AngleY="0" AngleX="4.415"/>
                            <RotateTransform Angle="-42.793"/>
                            <TranslateTransform/>
